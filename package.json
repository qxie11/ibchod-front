{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "docker:dev": "docker run -d -p 9002:9002 --name iphone-store-app iphone-store",
    "docker:build": "docker buildx build --platform linux/amd64,linux/arm64 -t dsx11/iobchod-store:latest --push .",
    "docker:push": "docker push dsx11/iobchod-store:latest",
    "docker:start": "docker start iphone-store",
    "docker:run": "./docker-run.sh",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --write",
    "prepare": "husky",
    "generate": "npx swagger-typescript-api generate --path  https://iobchod-server.vercel.app/api-json --name generated.ts --output src/generated",
    "lint-staged": "npx lint-staged"
  },
